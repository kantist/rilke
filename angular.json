{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"common": {
			"projectType": "library",
			"root": "projects/common",
			"sourceRoot": "projects/common",
			"prefix": "ril",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"configurations": {
						"production": {
							"tsConfig": "tsconfig.build.json",
							"project": "projects/common/ng-package.json"
						},
						"development": {
							"tsConfig": "tsconfig.build.json",
							"project": "projects/common/ng-package.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/common/src/test.ts",
						"tsConfig": "projects/common/tsconfig.spec.json",
						"karmaConfig": "projects/common/karma.conf.js"
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["**/*.ts", "**/*.html"]
					}
				}
			}
		},
		"ui": {
			"projectType": "library",
			"root": "projects/ui",
			"sourceRoot": "projects/ui",
			"prefix": "ril",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"configurations": {
						"production": {
							"tsConfig": "tsconfig.build.json",
							"project": "projects/ui/ng-package.json"
						},
						"development": {
							"tsConfig": "tsconfig.build.json",
							"project": "projects/ui/ng-package.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/ui/src/test.ts",
						"tsConfig": "projects/ui/tsconfig.spec.json",
						"karmaConfig": "projects/ui/karma.conf.js"
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["**/*.ts", "**/*.html"]
					}
				}
			}
		},
		"store": {
			"projectType": "library",
			"root": "projects/store",
			"sourceRoot": "projects/store",
			"prefix": "ril",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"configurations": {
						"production": {
							"tsConfig": "tsconfig.build.json",
							"project": "projects/store/ng-package.json"
						},
						"development": {
							"tsConfig": "tsconfig.build.json",
							"project": "projects/store/ng-package.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/store/src/test.ts",
						"tsConfig": "projects/store/tsconfig.spec.json",
						"karmaConfig": "projects/store/karma.conf.js"
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["**/*.ts", "**/*.html"]
					}
				}
			}
		}
	}
}
