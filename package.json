		"bundle-scss": "^1.5.1",
		"eslint": "8.21.0",
		"eslint-plugin-import": "2.26.0",
		"jasmine-core": "~4.3.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"ng-packagr": "^14.1.0",
		"prettier": "^2.4.1",
		"stylelint": "^14.0.1",
