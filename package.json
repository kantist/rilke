{
	"name": "rilke-workspace",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"*** DEV ***": "",
		"build:dev": "ng build --configuration development",
		"watch:dev": "ng build --watch --configuration development",
		"*** BUILD ***": "",
		"build": "ng build common && ng build ui && ng build cdk",
		"build:common": "ng build common",
		"watch:common": "ng build common --watch",
		"build:ui": "ng build ui",
		"watch:ui": "ng build ui --watch",
		"build:cdk": "ng build cdk",
		"watch:cdk": "ng build cdk --watch",
		"*** TEST ***": "",
		"test": "ng test",
		"*** LINTER ***": "",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"*** STYLELINT ***": "",
		"lint:styles": "stylelint \"**/*.scss\"",
		"lint:styles:fix": "stylelint \"**/*.scss\" --fix",
		"*** SCHEMATICS ***": "",
		"build:schematics": "cd projects/ui && npm run build"
	},
	"dependencies": {
		"@angular/animations": "~14.1.2",
		"@angular/cdk": "~14.1.2",
		"@angular/common": "~14.1.2",
